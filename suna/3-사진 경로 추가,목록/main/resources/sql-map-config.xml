<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias type="com.web.rnb.member.MemberDTO" alias="member"/>
		<typeAlias type="com.web.rnb.photo.PhotoDTO" alias="photo"/>
	 	<typeAlias type="com.web.rnb.photo.BoardPhotoDTO" alias="boardphoto"/>
		<typeAlias type="com.web.rnb.write.WriteDTO" alias="write"/>
		<typeAlias type="com.web.rnb.write.WritePager" alias="paging"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappings/member-mapping.xml"/>
		<mapper resource="mappings/photo-mapping.xml"/>
		<mapper resource="mappings/boardphoto-mapping.xml"/>
		<mapper resource="mappings/write-mapping.xml"/>
	</mappers>
	
</configuration>

